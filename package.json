{
  "name": "rentx",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --inspect --transpile-only --ignore-watch node_modules --respawn src/server.ts",
    "dev:typeorm": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "dev:migration:generate": "yarn dev:typeorm migration:generate --n",
    "dev:migration:run": "yarn dev:typeorm migration:run",
    "dev:migration:revert": "yarn dev:typeorm migration:revert",
    "typeorm": "./node_modules/typeorm/cli.js -f ./build/database/ormconfig.js"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/multer": "^1.4.5",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "prettier": "^2.2.1",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "csv-parse": "^4.15.4",
    "dotenv": "^9.0.0",
    "express": "^4.17.1",
    "multer": "^1.4.2",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.1.6",
    "tsyringe": "^4.5.0",
    "typeorm": "^0.2.32",
    "uuid": "^8.3.2"
  }
}
